<resources>

    <style name="NormalText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black_333333</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:drawableEnd">@drawable/ic_arrow_right</item>
        <item name="android:background">@color/white_ffffff</item>
    </style>

    <style name="NormalCheckBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black_333333</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:drawableEnd">@drawable/selector_cb_btn</item>
        <item name="android:background">@color/white_ffffff</item>
        <item name="android:button">@null</item>
    </style>
</resources>
